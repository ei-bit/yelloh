#include <stdio.h>

int main() {
	int N, M;
	scanf("%d\n", &N);
	if (N % 5 == 0) M = N / 5;
	if (N % 3 == 0) M = N / 3;
	if (N % 5 == 1) M = (N >= 6) ? (N / 5 + 1) : -1;
	if (N % 5 == 2) M = (N >= 12) ? (N / 5 + 2) : -1;
	if (N % 5 == 3) M = N / 5 + 1;
	if (N % 5 == 4) M = (N >= 9) ? (N / 5 + 2) : -1;
	
	printf("%d\n", M);
	return 0;
	/*
		1남으면(>=6) 5빼고 3두개넣기 (+1)
		2남으면(>=12) 5두개빼고 3네개 넣기 (+2)
		3남으면 3하나넣기 (+1)
		4남으면(>=9) 5하나 빼고 3 세개 넣기 (+2)
	*/

}
