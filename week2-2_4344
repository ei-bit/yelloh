#include <stdio.h>
int main() {
	int C, N, i, sum, cnt, a[1000];
	float av;
	scanf("%d", &C);
	while (C--) {
		scanf("%d", &N);
			for (i = 0, sum = 0; i < N; i++) {
				scanf("%d", &a[i]);
				sum += a[i];
			}
		av = (float)sum / (float)N;
		for (i = 0, cnt = 0; i < N; i++)
			if (a[i] > av) cnt++;
		printf("%.3f%%\n", ((float)cnt / N)*100.0);
	}
	return 0;
}
